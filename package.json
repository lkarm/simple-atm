  "author": "Lisa Armstrong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lkarm/simple-atm/issues"
  },
  "homepage": "https://github.com/lkarm/simple-atm#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "currency-formatter": "^1.2.1",
    "express": "^4.15.3",
    "html-webpack-plugin": "^2.30.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "redis": "^2.7.1",
    "redux": "^3.7.2"
