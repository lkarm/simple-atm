    "jest": "20.0.4",
    "mock-express-request": "^0.2.0",
    "mock-express-response": "^0.2.1",
    "prettier": "1.5.3",
    "redis-mock": "^0.20.0",
    "style-loader": "^0.18.2",
    "webpack-combine-loaders": "^2.0.3"
