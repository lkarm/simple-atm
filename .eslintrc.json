{
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "plugins": [
    "prettier"
  ],
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "rules": {
    "consistent-return": "warn",
    "strict": "warn",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}